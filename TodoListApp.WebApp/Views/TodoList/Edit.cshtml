@model (TodoListDto list, Uri url)
@{
    Layout = "_MinimalLayout";
}
<div class="container">
<partial name ="_TodoListEditor.cshtml" model='new TodoListEditorViewModel()
{
    List = @Model.list,
    CallbackMethodName = "Update",
    ThemeColor = "warning",
    Title = "Edit",
    ReturnUrl = @Model.url,
}'/>
</div>